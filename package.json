{
	"name": "@ubeac/svelte",
	"version": "0.0.3-next.3",
	"type": "module",
	"svelte": "index.js",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "npm run style && svelte-kit build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"package": "npm run style && svelte-kit package",
		"prepare": "svelte-kit sync",
		"preview": "svelte-kit preview",
		"publish": "npm run package && npm publish ./package",
		"start": "npm run style && svelte-kit dev",
		"style": "node scripts/styles.generator.js"
	},
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.14.3",
		"@iconify/svelte": "^2.2.1",
		"@popperjs/core": "^2.11.6",
		"@tabler/core": "^1.0.0-beta11",
		"bootstrap": "^5.1.3",
		"change-case": "^4.1.2",
		"litepicker": "^2.0.12",
		"moment": "^2.29.4",
		"nanoid": "^3.3.4",
		"tom-select": "^2.1.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@trivago/prettier-plugin-sort-imports": "^3.2.0",
		"@types/bootstrap": "^5.1.10",
		"@types/luxon": "^2.3.2",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"sass": "^1.51.0",
		"svelte": "^3.47.0",
		"svelte-check": "^2.2.6",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.10.1",
		"svelte2tsx": "^0.5.10",
		"tslib": "^2.3.1",
		"typescript": "^4.6.2"
	}
}

{
	"name": "@ubeac/svelte",
	"version": "0.0.1",
	"type": "module",
	"svelte": "index.js",
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"package": "npm run style && svelte-kit package",
		"publish": "npm run package && npm publish ./package",
		"start": "npm run style && svelte-kit dev",
		"sb": "npm run storybook:start",
		"sb:build": "storybook:build",
		"storybook:start": "set NODE_OPTIONS=--openssl-legacy-provider && npm run style && start-storybook -p 6006",
		"storybook:build": "set NODE_OPTIONS=--openssl-legacy-provider && npm run style && build-storybook -o ./.storybook/build",
		"style": "node scripts/styles.generator.js"
	},
	"dependencies": {
		"@googlemaps/js-api-loader": "^1.14.3",
		"@tabler/core": "^1.0.0-beta11",
		"@ubeac/svelte-components": "^0.0.40",
		"@iconify/svelte": "^2.2.1",
		"@popperjs/core": "^2.11.5",
		"bootstrap": "^5.1.3",
		"change-case": "^4.1.2",
		"litepicker": "^2.0.12",
		"luxon": "^2.4.0",
		"nanoid": "^3.3.4",
		"tom-select": "^2.0.3"
	},
	"devDependencies": {
		"@babel/core": "^7.17.9",
		"@storybook/addon-actions": "^6.4.22",
		"@storybook/addon-essentials": "^6.4.22",
		"@storybook/addon-links": "^6.4.22",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/addon-svelte-csf": "^2.0.3",
		"@storybook/builder-vite": "^0.1.30",
		"@storybook/svelte": "^6.4.22",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "1.0.0-next.405",
		"@trivago/prettier-plugin-sort-imports": "^3.2.0",
		"@types/bootstrap": "^5.1.10",
		"@types/cookie": "^0.5.1",
		"@types/google.maps": "^3.49.2",
		"@types/luxon": "^2.3.2",
		"autoprefixer": "^10.4.8",
		"babel-loader": "^8.2.5",
		"postcss-cli": "^10.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.51",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-loader": "^3.1.2",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.10",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "3"
	}
}
